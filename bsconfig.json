{
  "name": "jvlk-api",
  "jsx": { "version": 4, "mode": "automatic" },
  "sources": [
    {
      "dir": "src",
      "subdirs": true,
      "public": "all"
    },
    {
      "dir": "tests",
      "subdirs": true,
      "type": "dev"
    }
  ],
  "package-specs": [
    {
      "module": "commonjs",
      "in-source": true
    }
  ],
  "suffix": ".js",
  "bs-dependencies": [
    "@rescript/react",
    "@rescript/core",
    "@dusty-phillips/rescript-zora",
    "rescript-struct",
    "rescript-envsafe"
  ],
  "bs-dev-dependencies": [],
  "bsc-flags": ["-open RescriptCore"]
}
